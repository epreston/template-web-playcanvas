{
  "private": true,
  "type": "module",
  "name": "playcanvas-template",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "Chrome >= 112",
    "ChromeAndroid >= 112",
    "Edge >= 112",
    "Firefox >= 112",
    "FirefoxAndroid >= 112",
    "Safari >= 16.4",
    "iOS >= 16.4",
    "not dead"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "start": "vite --open",
    "lint": "eslint --ext .js,.ts --report-unused-disable-directives .",
    "format": "prettier --check --cache \"**/*.[tj]s?(x)\"",
    "format-fix": "prettier --write --cache \"**/*.[tj]s?(x)\""
  },
  "devDependencies": {
    "@playcanvas/eslint-config": "^1.7.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.4"
  },
  "dependencies": {
    "playcanvas": "^1.67.0"
  },
  "engines": {
    "node": "^18.18.0 || >=20.9.0"
  }
}
